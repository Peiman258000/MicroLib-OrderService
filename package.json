{
  "name": "federated-monolith-services",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "installConfig": {
    "pnp": false
  },
  "repository": {
    "url": "https://github.com/tysonrm/composable-microservices-remotes"
  },
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js",
    "start": "node -r dotenv/config dist/main.js",
    "dev-start": "nodemon --watch src --exec npm restart",
    "restart": "npm run build && npm run start",
    "clean": "rm -rf dist",
    "transpile": "babel src -d dist",
    "test": "",
    "test-remote": "mocha",
    "demo": "open http://localhost:8060",
    "kafka-zookpr": "../kafka/bin/zookeeper-server-start.sh  ../kafka/config/zookeeper.properties",
    "kafka-server": "../kafka/bin/kafka-server-start.sh ../kafka/config/server.properties",
    "kafka-topics-order": "../kafka/bin/kafka-topics.sh --if-not-exists --create --topic orderChannel --bootstrap-server localhost:9092",
    "kafka-topics-shipping": "../kafka/bin/kafka-topics.sh --if-not-exists --create --topic shippingChannel --bootstrap-server localhost:9092",
    "kafka-topics-inventory": "../kafka/bin/kafka-topics.sh --if-not-exists --create --topic inventoryChannel --bootstrap-server localhost:9092"
  },
  "author": "Tyson Midboe",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "kafkajs": "^1.14.0",
    "smartystreets-javascript-sdk": "^1.6.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-register": "^6.26.0",
    "esm": "^3.2.25",
    "express-cli": "0.0.1",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "nodemon-webpack-plugin": "^4.3.2",
    "webpack": "^5.0.0-beta.28",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
